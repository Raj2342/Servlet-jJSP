<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<display-name>XmlWebDemo</display-name>
     <!-- login --> 
      <servlet-mapping>
          <servlet-name>  mlogin </servlet-name>
          <url-pattern>/login</url-pattern>
      </servlet-mapping>
      
      <servlet>
      <servlet-name> mlogin </servlet-name>
      <servlet-class>  in.sp.backend.login</servlet-class>
      </servlet>
      
       <!-- register -->
       
       <servlet-mapping>
         <servlet-name>regist</servlet-name>
         <url-pattern>/myregister</url-pattern>
       </servlet-mapping>
       
       <servlet>
       <servlet-name> regist </servlet-name>
      <servlet-class>  in.sp.backend.Register</servlet-class>
       </servlet>

</web-app>